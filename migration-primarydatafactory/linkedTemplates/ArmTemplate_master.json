{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "migration-primarydatafactory"
		},
		"Storage_destination_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Storage_destination'"
		},
		"source_mysql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'source_mysql'"
		},
		"AzureSynapseArtifacts1_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://migration-synapse.dev.azuresynapse.net"
		},
		"Storage_destination_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://migrationprimarystorage.dfs.core.windows.net/"
		},
		"source_mysql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "192.168.200.200"
		},
		"source_mysql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "migration_database"
		},
		"source_mysql_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "rinith"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "migration-primarydatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Storage_destination_accountKey": {
						"value": "[parameters('Storage_destination_accountKey')]"
					},
					"source_mysql_password": {
						"value": "[parameters('source_mysql_password')]"
					},
					"AzureSynapseArtifacts1_properties_typeProperties_endpoint": {
						"value": "[parameters('AzureSynapseArtifacts1_properties_typeProperties_endpoint')]"
					},
					"Storage_destination_properties_typeProperties_url": {
						"value": "[parameters('Storage_destination_properties_typeProperties_url')]"
					},
					"source_mysql_properties_typeProperties_server": {
						"value": "[parameters('source_mysql_properties_typeProperties_server')]"
					},
					"source_mysql_properties_typeProperties_database": {
						"value": "[parameters('source_mysql_properties_typeProperties_database')]"
					},
					"source_mysql_properties_typeProperties_username": {
						"value": "[parameters('source_mysql_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}