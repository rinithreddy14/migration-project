{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "migration-primarydatafactory"
		},
		"Storage_destination_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Storage_destination'"
		},
		"source_mysql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'source_mysql'"
		},
		"Storage_destination_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://migrationprimarystorage.dfs.core.windows.net/"
		},
		"source_mysql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "192.168.200.200"
		},
		"source_mysql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "migration_database"
		},
		"source_mysql_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "rinith"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Storage_destination')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('Storage_destination_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('Storage_destination_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/source_mysql')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "MySql",
				"typeProperties": {
					"server": "[parameters('source_mysql_properties_typeProperties_server')]",
					"port": 3306,
					"database": "[parameters('source_mysql_properties_typeProperties_database')]",
					"username": "[parameters('source_mysql_properties_typeProperties_username')]",
					"sslMode": 1,
					"useSystemTrustStore": 0,
					"password": {
						"type": "SecureString",
						"value": "[parameters('source_mysql_password')]"
					},
					"driverVersion": "v2"
				},
				"connectVia": {
					"referenceName": "mysql-intergration-runtime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/mysql-intergration-runtime')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/mysql-intergration-runtime')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/hourlysteps_merged')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "source_mysql",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "source"
				},
				"annotations": [],
				"type": "MySqlTable",
				"schema": [],
				"typeProperties": {
					"tableName": "`hourlysteps_merged`"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/source_mysql')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/minutemetsnarrow_merged')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "source_mysql",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "source"
				},
				"annotations": [],
				"type": "MySqlTable",
				"schema": [],
				"typeProperties": {
					"tableName": "`minutemetsnarrow_merged`"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/source_mysql')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/minuteintensitiesnarrow_merged')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
