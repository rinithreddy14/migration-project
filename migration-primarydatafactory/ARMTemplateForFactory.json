				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/source_mysql')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/minutecaloriesnarrow_merged')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "source_mysql",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "source"
				},
				"annotations": [],
				"type": "MySqlTable",
				"schema": [],
				"typeProperties": {
					"tableName": "`minutecaloriesnarrow_merged`"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/source_mysql')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/heartrate_seconds_merged')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "source_mysql",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "source"
				},
				"annotations": [],
				"type": "MySqlTable",
				"schema": [],
				"typeProperties": {
					"tableName": "`heartrate_seconds_merged`"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/source_mysql')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/hourlyintensities_merged')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "source_mysql",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "source"
				},
				"annotations": [],
				"type": "MySqlTable",
				"schema": [],
				"typeProperties": {
					"tableName": "`hourlyintensities_merged`"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/source_mysql')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/hourlycalories_merged')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "source_mysql",
					"type": "LinkedServiceReference"
				},
				"folder": {
